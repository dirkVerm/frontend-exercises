source:"var bluecore = {\n  loaded : false,\n  triggerSignupLoaded : false,\n  loadLibrary : function() {\n    if (dctfl.affiliateCheck(dctfl.domain, 'bluecore') === true && bluecore.loaded === false) {\n      bluecore.loaded = true;\n      try {\n        if (window.performance) { window.performance.mark('adobelaunch_bluecore_loadlibrary'); }\n      } catch (err) { dctfl.log(err); }\n      try {\n        var bcConfig = dctfl.accounts({'domain' : dctfl.domain, 'affiliate' : 'bluecore'}),\n            clientID = '';\n        if (typeof bcConfig === 'object') {\n          clientID = bcConfig.client;\n        } else {\n          clientID = bcConfig;\n        }\n\n        document.addEventListener('bluecore:falconOnload', dctfl.bluecore.bcSitesReadyEvent);\n\n        var triggermail=window.triggermail||{};\n        triggermail.load=function(a){ \n          var b=document.createElement(\"script\");\n          b.type=\"text/javascript\"; \n          b.async=!0;\n          b.src=\"https://api.bluecore.com/triggermail.js/\"+a+\".js\"; \n          a=document.getElementsByTagName(\"script\")[0];\n          a.parentNode.insertBefore(b,a)\n        }; \n        triggermail.load(clientID);\n        window.triggermail=triggermail;\n\n        // handle entry pageView for specific page types\n        var pageType = _satellite.getVar('pageType') || '';\n        pageType = pageType.toLowerCase();\n        if (!dctfl.findInList(pageType, ['confirm', 'cart', 'product', 'search', 'brand', 'null search']) && pageType.indexOf('category') === -1) {\n          bluecore.trackEvent('pageView', { pageName : _satellite.getVar('pageName') });\n        }\n\n        if (typeof bcConfig === 'object' && 'sites' in bcConfig) {\n          // load sites triggers\n          dctfl.dependsLoaded('bluecore1', 'object', function() {\n            dctfl.dependsLoaded('bluecoretrigger', 'function', function() {\n              bluecore.loadSitesTriggerSignup();\n            }, 'sites');\n          }, 'sites');\n        }        \n      } catch (e) {\n        dctfl.log(e);\n      }\n    } else {\n      return false;\n    }\n  },\n  bcSitesReadyEvent : function() {\n    dctfl.log('@bluecore sites ready');\n\n    if (dctfl.initialEntryPageUrl && dctfl.initialEntryPageUrl.indexOf(\"bluecore_triggerload=1\") !== -1) {\n      var currentSitesConfig = dctfl.bluecore.getSitesTriggerConfig();\n      if (currentSitesConfig) {\n        var bcSitesCampaignId = triggermail.util.isMobile() ? currentSitesConfig.mobile : currentSitesConfig.desktop;\n        dctfl.log('@bluecore queryString triggering sites campaign', bcSitesCampaignId);\n        dctfl.bluecore.triggerSitesCampaign(bcSitesCampaignId);\n      } else {\n        dctfl.log('@bluecore sites modal trigger not configured');\n      }\n      document.removeEventListener('bluecore:falconOnload', dctfl.bluecore.bcSitesReadyEvent);\n    }\n    \n  },\n  testActive : function(name, experience) {\n    var isTestActive = false;\n    if (typeof window.bluecore === 'object') {\n        if (window.bluecore.tests && window.bluecore.tests.length > 0) {\n            for (var i=0; i<window.bluecore.tests.length; i++) {\n                var obj = window.bluecore.tests[i];\n                if (obj.name == name && obj.experience == experience) {\n                  isTestActive = true;\n                  break;\n                }\n            }\n        }\n    }\n    return isTestActive;\n  },\n  triggerSitesCampaign: function (campaignId) {\n    if (dctfl.affiliateCheck(dctfl.domain, 'bluecore') === true) {\n      if (campaignId) {\n        dctfl.dependsLoaded('bluecoretrigger', 'function', function () {\n          bluecore_action_trigger(campaignId);\n        }, 'callout');\n        return true;\n      }\n    }\n    return false;\n  },\n  getSitesTriggerConfig : function() {\n    var language = _satellite.getVar('language') || 'en',\n      bcSitesConfig = dctfl.accounts({ 'domain': dctfl.domain, 'affiliate': 'bluecore' }).sites,\n      bcSitesId = false;\n\n    if (typeof bcSitesConfig === 'object' &&\n      typeof bcSitesConfig.triggersignup === 'object' &&\n      language in bcSitesConfig.triggersignup) {\n        // ensure both desktop and mobile are configured\n        if (bcSitesConfig.triggersignup[language].mobile && bcSitesConfig.triggersignup[language].desktop) {\n          bcSitesId = bcSitesConfig.triggersignup[language];\n        }\n    }\n    \n    return bcSitesId;\n  },\n   loadSitesTriggerSignup: function () {\n    if (\n      dctfl.affiliateCheck(dctfl.domain, \"bluecore\") === true &&\n      bluecore.triggerSignupLoaded === false\n    ) {\n      bluecore.triggerSignupLoaded = true;\n      var language = _satellite.getVar(\"language\") || \"en\",\n          bcSitesId = bluecore.getSitesTriggerConfig(),\n          footerCallouts = document.querySelectorAll('footer .FooterCallout'),\n          emailSignUpPos = 0,\n          hasCallouts = footerCallouts.length > 0,\n          footerNodesInPlace = true;\n\n        \n        if (hasCallouts) {\n          if (footerCallouts.length > 2) {\n            emailSignUpPos = 2; // we will default to the 3rd position \n          } else {\n            if (footerCallouts.length > 0) {\n              emailSignUpPos = footerCallouts.length; // push to the end\n            }\n          }\n        }\n\n      if (!bcSitesId) {\n        dctfl.log(\"@bluecore sites modal trigger not configured\");\n      } else {\n        dctfl.log(\"@bluecore sites modal trigger ids\", bcSitesId);\n\n        // TODO: move this to a global language config\n        var calloutConfig = {\n          copy: {\n            en: {\n              title: \"Email signup\",\n              text: \"Get news, offers, and discounts.\",\n              button: \"SUBSCRIBE\",\n            },\n            nl: {\n              title: \"E-mail voor aanmelden\",\n              text: \"Ontvang nieuws, aanbiedingen en kortingen.\",\n              button: \"ABONNEREN\",\n            },\n            de: {\n              title: \"Anmeldung per E-Mail\",\n              text: \"Erhalte Neuigkeiten, Angebote und Rabatte.\",\n              button: \"ABONNIEREN\",\n            },\n            es: {\n              title:\n                \"Suscribirse con la direcci\\u00f3n de correo electr\\u00f3nico\",\n              text: \"Recibe noticias, ofertas y descuentos.\",\n              button: \"SUSCR\\u00cdBETE\",\n            },\n            it: {\n              title: \"Registrati via e-mail\",\n              text: \"Ricevi notizie, offerte e sconti.\",\n              button: \"REGISTRATI\",\n            },\n            fr: {\n              title: \"Recevez nos e-mails\",\n              text: \"Recevez l'actualit\\u00e9, des offres sp\\u00e9ciales et des r\\u00e9ductions.\",\n              button: \"M'INSCRIRE\",\n            },\n          }\n       };\n        if (_satellite.getVar(\"isNA\")) {\n          // updated EN copy for NA\n          calloutConfig.copy.en = {\n            title: \"Sign Up For Emails\",\n            text: \"Stay in the loop, wherever you go\",\n            button: \"Sign Up For Emails\",\n          };\n\n          // CA FR:\n          calloutConfig.copy.fr = {\n            title: \"Inscrivez-vous pour les emails\",\n            text: \"Restez au courant - inscrivez-vous pour recevoir les derniers courriels de Foot Locker\",\n            button: \"Apprendre encore plus\",\n          };\n        }\n\n        if (language in calloutConfig.copy) {\n          window.addEventListener('reduxAction', function(e) {\n              var reduxType = e.detail.type;\n              if (reduxType === 'EVENT_ADD_LINK') {\n                  var linkPayload = e.detail.payload;\n                  if (linkPayload.to === \"#bluecoreFooterLink\"){\n                      setTimeout(function () {\n                          var closeButton = document.querySelector(\n                            \".ReactModalPortal .c-header-navigation-drawer-modal .c-modal__close\"\n                          );\n                          if (closeButton) {\n                            closeButton.click();\n                            dctfl.log(\"@bluecore mobile menu drawer closed\");\n                          }\n                        }, 1);\n      \n                      setTimeout(function () {\n                        var currentSitesConfig = bluecore.getSitesTriggerConfig();\n                        if (currentSitesConfig) {\n                          var bcSitesCampaignId = triggermail.util.isMobile()\n                            ? currentSitesConfig.mobile\n                            : currentSitesConfig.desktop;\n                          dctfl.log(\n                            \"@bluecore triggering sites campaign\",\n                            bcSitesCampaignId\n                          );\n                          bluecore.triggerSitesCampaign(bcSitesCampaignId);\n                        } else {\n                          dctfl.log(\"@bluecore sites modal trigger not configured\");\n                        }\n                      }, 1);\n          \n                  }\n              }\n          });       \n          window.footlocker.editFooterComponent({\n            editType: \"add\",\n            position: emailSignUpPos,\n            sectionName: \"callouts\",\n            data: {\n              desc: calloutConfig.copy[language].text,\n              icon: \"ic_message\",\n              isNewWindow: false,\n              linkdesc: calloutConfig.copy[language].button,\n              linkurl: \"#bluecoreFooterLink\",\n              title: calloutConfig.copy[language].title,\n            },\n          });\n\n         \n        } else {\n          dctfl.log(\n            \"@bluecore current language\",\n            language,\n            \"not configured, exiting email sign up mods\"\n          );\n        }\n      }\n    }\n  },\n  trackPurchasePixel : function(clientID, obj) {\n    var promoCodes = _satellite.getVar('getTransactionPr