ary is never called more than once\n    if (riskified.state === -1) {\n      if (riskified.cartId()) {\n        riskified.loadLibrary();\n      }\n    }\n  },\n  loadLibrary: function() {\n    riskified.state = 0; // loading\n    dctfl.log('Riskified: init');\n    try {\n      if (window.performance) { window.performance.mark('adobelaunch_riskified_loadlibrary'); }\n    } catch (err) { dctfl.log(err); }\n    var storeDomain = location.host.replace('www.', ''),\n      sessionID = riskified.cartId(),\n      url = ('https:' == document.location.protocol ? 'https://' : 'http://') +\n        \"beacon.riskified.com?shop=\" + storeDomain + \"&sid=\" + sessionID,\n      s = document.createElement('script'),\n      x = document.getElementsByTagName('script')[0];\n    s.type = 'text/javascript';\n    s.async = true;\n    s.src = url;\n    x.parentNode.insertBefore(s, x);\n\n    dctfl.dependsLoaded('riskified', 'object', function() {\n      riskified.state = 1; // loaded\n      dctfl.log('Riskified: loaded');\n    }, 'load');\n  },\n  cartId: function() {\n    if (!riskified.lastCartID) {\n      if (digitalData && digitalData.cart && digitalData.cart.cartID) {\n        riskified.lastCartID = digitalData.cart.cartID;\n      } else if (_satellite.cookie.get('cart-guid')) {\n        digitalData.cart.cartID = _satellite.cookie.get('cart-guid');\n      }\n    }\n    return riskified.lastCartID;\n  },\n  lookupCartId: function(callback) {\n    // digitalData isn't always updated when the cart merge happens so we are going to the API\n    // to get the cart id\n    dctfl.apiRequest('/api/users/carts/current?checkInventory=true&checkPriceVariation=true&timestamp=' + (new Date()).getTime()).then(function(data) {\n      if (data && data.guid) {\n        dctfl.log('Riskified cart id:', data.guid);\n        riskified.lastCartID = data.guid;\n        callback(data.guid);\n      }\n    })['catch'](function(err) {\n      dctfl.log(err);\n    });\n  },\n  trackEvent: function(event) {\n    if