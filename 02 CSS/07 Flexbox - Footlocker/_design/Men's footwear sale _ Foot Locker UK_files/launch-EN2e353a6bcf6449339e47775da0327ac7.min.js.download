   (function(win, doc, sdk_url){\n        if(win.snaptr) return;\n        var tr=win.snaptr=function(){\n          tr.handleRequest? tr.handleRequest.apply(tr, arguments):tr.queue.push(arguments);\n        };\n        tr.queue = [];\n        var s='script';\n        var new_script_section=doc.createElement(s);\n        new_script_section.async=!0;\n        new_script_section.src=sdk_url;\n        var insert_pos=doc.getElementsByTagName(s)[0];\n        insert_pos.parentNode.insertBefore(new_script_section, insert_pos);\n      })(window, document, 'https://sc-static.net/scevent.min.js');\n      dctfl.dependsLoaded('snapchat', 'function', function() {\n        var emailAddress = _satellite.getVar('userEmailAddress') || '';\n        snaptr('init', dctfl.accounts({'domain' : dctfl.domain, 'affiliate' : 'snapchat'}),{ \n          user_email : emailAddress\n        });\n\n        // handle entry pageView for specific page types\n        var pageType = _satellite.getVar('pageType') || '';\n        pageType = pageType.toLowerCase();\n        if (!dctfl.findInList(pageType, ['confirm', 'product', 'search', 'brand', 'null search']) && pageType.indexOf('category') === -1) {\n          snaptr('track', 'PAGE_VIEW', { \n            description : digitalData.page.pageInfo.pageName\n          });            \n        }\n\n      }, 'load');\n    } else {\n      return false;\n    }\n  },\n  trackEvent : function(eventType, obj) {\n    if (dctfl.affiliateCheck(dctfl.domain, 'snapchat') === true) {\n      dctfl.dependsLoaded('snapchat', 'function', function() {\n        switch (eventType) {\n          case 'PAGE_VIEW':\n            snaptr('track', 'PAGE_VIEW', { \n              description : obj.pageName\n            });\n            break;\n          case 'VIEW_CONTENT': // product\n            snaptr('track', 'VIEW_CONTENT', { \n              item_category: obj.categories, \n              item_ids: [ obj.sku ]\n            }); \n            break;\n          case 'SEARCH':\n         