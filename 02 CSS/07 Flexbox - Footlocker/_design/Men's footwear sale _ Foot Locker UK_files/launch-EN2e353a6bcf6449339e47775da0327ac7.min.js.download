source:'var awin = { \n    accountId : dctfl.accounts({\'domain\' : dctfl.domain, \'affiliate\' : \'awin\'}),\n    loadLibrary : function() {\n        awin.awinBaseScript = document.createElement("script");\n        awin.awinBaseScript.type = "text/javascript";\n        awin.awinBaseScript.src = "https://www.dwin1.com/" + awin.accountId + ".js";\n        awin.awinBaseScript.id = "awin-master";\n        awin.awinBaseScript.onerror = function(err) {\n          var pfCookies = _satellite.getVar(\'PerformanceCookiesEnabled\');\n          if(pfCookies === true && \'google360\' in dctfl) dctfl.google360.trackEvent(\'exception\', {message: \'AWIN - Load_Error - \' + JSON.stringify(err)});\n        };\n        awin.awinBaseScript.defer = true;\n        document.head.appendChild(awin.awinBaseScript);\n    },\n    referrer: function() {\n      var referrer = "na";\n      var availReferrers = [\'awin\', \'google\', \'criteo\', \'everysize\', \'idealo\', \'bing\', \'complex\', \'dbm\', \'programmatic\', \'instagram\', \'facebook\', \'fb\'];\n      var lastReferrer = _satellite.getVar(\'lastChannel\')\n      if (lastReferrer) {\n          lastReferrer = JSON.parse(lastReferrer)\n          if (lastReferrer && Object.keys(lastReferrer).length && ((new Date().getTime() - lastReferrer.t) / 1000 / 60 / 60 / 24) < 30) {\n              if (lastReferrer.c === \'r\') {\n                  referrer = \'referral\'\n              } else if (lastReferrer.r.indexOf(\'utm_medium=email\') !== -1) {\n                  referrer = \'email\'\n              } else if (lastReferrer.r.indexOf(\'utm_source=fb\') !== -1 || lastReferrer.r.indexOf(\'utm_source=ig\') !== -1) {\n                  referrer = \'facebook\'\n              } else if (lastReferrer.r.indexOf(\'utm_source=pla\') !== -1 || lastReferrer.r.indexOf(\'gclid\') !== -1) {\n                referrer = \'google\'\n              } else if (lastReferrer.r.indexOf(\'utm_source=idealo.de\') !== -1) {\n                  referrer = \'idealo\'\n              } else if (lastReferrer.r.indexOf(\'utm_source=awin\') !== -1) {\n                  referrer = \'aw\'\n              } else if (lastReferrer.r.indexOf(\'utm_source\') !== -1) {\n                  var utmSource = \'\';\n                  var subStr = lastReferrer.r.substr(lastReferrer.r.indexOf(\'utm_source\'))\n                  var end = subStr.indexOf(\'&\');\n                  if (end !== -1) {\n                      utmSource = subStr.substr(0, end)\n                  } else {\n                      utmSource = subStr.substr(0)\n                  }\n                  utmSource = utmSource.split(\'=\');\n                  if (availReferrers.indexOf(utmSource[1]) !== -1) {\n                      referrer = utmSource[1]\n                  } else {\n                      referrer = \'referral\'\n                  }\n              } else if (lastReferrer.r.indexOf(\'utm_medium\') !== -1) {\n                referrer = \'referral\'\n              }\n          }\n      }\n      return referrer;\n    },\n    trackConversion : function(obj) {\n      var ddTransaction = _satellite.getVar(\'digitalDataTransaction\');\n      var transactionDetailsDE = _satellite.getVar(\'getTransactionDetails\');\n      var promoCodes = _satellite.getVar(\'getTransactionPromoCodes\');\n      var city = \'\';\n      var promoCode = \'\';\n      if (ddTransaction){\n          try {\n            city = ddTransaction.profile.shippingAddress.line1\n          } catch (e){\n            dctfl.log(e)\n          }\n      }\n      if (promoCodes.length){\n        promoCode = promoCodes[0]\n      }\n      if (_satellite.getVar(\'isEUorAPAC\')){\n         var skuPriceNetArr=[];\n         var entries = transactionDetailsDE.items\n         for(var i=0; i<entries.length; i++){\n              if (entries[i].price.value > 0){\n                  var prodSku = entries[i].productInfo.productID\n                  skuPriceNetArr[prodSku] = entries[i].price\n              }\n          }\n          ddTransaction.items.forEach(function(item){\n                  var sku = item.productInfo.productID\n                  item.priceWtax = item.price\n                  item.price = skuPriceNetArr[sku]\n          })\n      }\n   \n      var accountId = awin.accountId;\n      var _sysVarsCoupon = promoCode;\n      var _sysVarsProducts = ddTransaction.items;\n      var _sysVarsId = ddTransaction.transactionID;\n      var _sysVarsCurrency = ddTransaction.total.currency;\n      var _sysVarsRevNet = _satellite.getVar("getTransactionDetails").orderTotal;\n      var _sysVarsUserid = _satellite.getVar("transactionUserEmail");\n      var _sysVarsShippingmethod = _satellite.getVar(\'transactionShippingMethod\');\n      var _sysVarsCustomerCountry = _satellite.getVar(\'countryCode\');\n      var _sysVarsCustomerCity = city;\n      var _sysVarsShipping = _satellite.getVar(\'transactionShippingAmount\');\n      var _sysVarsPaymentmethod = _satellite.getVar(\'transactionPaymentMethod\');\n      var _sysVarsCustomerGender = _satellite.getVar(\'firstProductGenderAffinity\');\n      var testmode = dctfl.debug === true ? 1 : 0;\n      var _yeezy_key = "NO_COMMISSION"; \n      var _yeezy_identifier = "yeezy";\n      var productDelimiter="AW:P|"+accountId;\n      var paramDelimiter = "|";\n      var lineBreak = "\\r\\n";\n      var productTrackingList = calculateIndividualCommission();\n      var awinForm = document.createElement(\'form\');\n      awinForm.style.display = \'none\';\n      awinForm.name = \'aw_basket_form\';\n      var textArea = document.createElement(\'textarea\');\n      textArea.wrap = \'physical\';\n      textArea.id = \'aw_basket\';\n      textArea.innerHTML = JSON.stringify(productTrackingList);\n      awinForm.appendChild(textArea);\n      var body = document.querySelector(\'body\');\n      body.append(awinForm);\n\n      function getvoucherPrefix()\n      {\n          if(!_sysVarsCoupon || _sysVarsCoupon === "")\n          {\n              return "novoucher";\n          