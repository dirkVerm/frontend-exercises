source:"function checkCartStateUpdate(counter, callback){\n    if (counter > 0){\n        var state = footlocker.globalstore.getState();\n        if (state && state.cart && state.cart.current){\n            var currentCart = state.cart.current\n            var totalPrice = currentCart.totalPrice.value\n            var totalPriceWithTax = currentCart.totalPriceWithTax.value\n            if (totalPriceWithTax !== digitalData.cart.price.priceWithTax){\n                window.setTimeout(function() {\n                    counter--\n                    checkCartStateUpdate(counter, callback)\n                }, 100);\n            } else {\n                callback()\n            }\n        }\n    }\n}\n\nfunction checkDDupdate(counter, callback){\n    if (counter > 0){\n        if (window.digitalData.cart && window.digitalData.cart.items.length > 0){\n            if (window.digitalData.cart.items[0].productInfo.sku === undefined){\n                window.setTimeout(function() {\n                    counter--\n                    checkDDupdate(counter, callback)\n                }, 200);\n            } else {\n                callback()\n            }\n        }\n    }\n}\n\nwindow.addEventListener('reduxAction', function(e) {\n    var reduxType = e.detail.type;\n    dctfl.log(\"reduxType\", reduxType, e);\n    var isEUorAPAC = _satellite.getVar('isEUorAPAC');\n  \n    switch (reduxType) {\n      case 'SET_PAYMENT_STATUS': \n          setTimeout(function() { \n              _satellite.track('checkout:payment:set', e.detail.payload);\n          }, dctfl.eventDelay);\n      break;\n      case 'STORE_LOCATOR_SUCCESS':\n        try {\n          if (e.detail.payload && e.detail.payload.stores) {\n            var storeResultCount = e.detail.payload.stores.length;\n            if (storeResultCount > 0) {\n              setTimeout(function() { \n                _satellite.track('bopis:search:success', e.detail.payload);\n              }, dctfl.eventDelay);\n            } else {\n              setTimeout(function() { \n                _satellite.track('bopis:search:fail', e.detail.payload);\n              }, dctfl.eventDelay);\n            }\n          }\n        } catch (err) {\n          dctfl.log(err);\n        }\n        break;\n      case 'STORE_LOCATOR_FAIL':\n        _satellite.track('bopis:search:fail', e.detail.payload);\n        break;\n      case 'EVENT_ADD_LINK': \n        setTimeout(function() {\n          _satellite.track('link:click', e.detail.payload);\n          window.dispatchEvent(new CustomEvent('analyticsEvent', {\n            detail: {\n              eventName: 'click',\n              payload: e.detail.payload\n            },\n            bubbles: true,\n            cancelable: true\n          }));\n        }, dctfl.eventDelay);\n        break;\n      case 'CHECKOUT_SET_MARKETING_OPT_IN': \n        if (e.detail.payload === true) {\n          setTimeout(function() { \n            _satellite.track('checkout:marketing:optin:true', {});\n          }, dctfl.eventDelay); \n        } else if (e.detail.payload === false) {\n          setTimeout(function() { \n            _satellite.track('checkout:marketing:optin:false', {});\n          }, dctfl.eventDelay); \n        }\n        break;\n      case 'CHECKOUT_SET_TERMS_AND_CONDITIONS': \n        if (e.detail.payload === true) {\n          setTimeout(function() { \n            _satellite.track('checkout:termsconditions:accept', {});\n          }, dctfl.eventDelay);\n        }\n        break;\n      case 'CHECKOUT_SET_CONTACT_INFO':\n        setTimeout(function() { \n          _satellite.track('checkout:contact:set', {});\n        }, dctfl.eventDelay);\n        break;\n      case 'PRODUCT_DETAILS_SELECT_VALUE':\n  \n        var pageType = _satellite.getVar('pageType') || '';\n        pageType = pageType.toLowerCase();\n  \n        if (pageType === 'product') {\n  \n          var pdpStyleSkus = dctfl.getPageInteractions('pdpSkus') || [];\n          // SPA event for PDP SKU selected, style selected, size selected\n          if (pdpStyleSkus && pdpStyleSkus.length > 0) {\n  \n            // check selected from state before sending events\n            // address the scenario where on initial enty this event fires\n            var sendProductEvents = true,\n                isPageLoad = dctfl.locationCount > 1 ? false : true;\n            try {\n              var state = footlocker.globalstore.getState();\n              if (typeof state.details.selected !== 'object' || typeof state.details.selected[location.pathname] !== 'object') {\n                sendProductEvents = false;\n              }\n            } catch (err) {\n              dctfl.log(err);\n            }\n  \n            if (sendProductEvents) {\n              dctfl.log('@product change : PRODUCT_DETAILS_SELECT_VALUE');\n              if (dctfl.debug === true) {\n                console.log('@product payload', e.detail.payload);\n                console.log('@product details state', footlocker.globalstore.getState().details);\n              }\n              \n              if (e.detail.payload.variantAttributes) {\n                // excluding for now  \n              } else {\n  \n                if (e.detail.payload.style) {\n                  var prodObj = {\n                    sku : e.detail.payload.style.sku\n                  };\n  \n                  var sizeCount = 0,\n                      productData = _satellite.getVar('getProductDetails'),\n                      pdpSizeInteractions = dctfl.getPageInteractions('pdpSizeSelected') || [],\n                      lastSizeSelected;\n  \n                  if (productData) {\n                    sizeCount = (productData.sizes ? productData.sizes.length : 0);\n                  }\n                    \n                  if (pdpSizeInteractions && pdpSizeInteractions.length > 0) {\n                      lastSizeSelected = pdpSizeInteractions[pdpSizeInteractions.length - 1];\n                  }\n  \n                  if (e.detail.payload.size && e.detail.payload.size.size && sizeCount > 0 && lastSizeSelected != e.detail.payload.size.size.value) {\n                    dctfl.log('@product size event source : PRODUCT_DETAILS_SELECT_VALUE');\n                    // PDP size selected\n                    var sizeValue = e.detail.payload.size.size.value,\n                        sizeBOSSDisabled = false;\n  \n                    if ('singleStoreInventory' in e.detail.payload.size && 'sizeAvailableInStores' in e.detail.payload.size) {\n                      sizeBOSSDisabled = e.detail.payload.size.singleStoreInventory === true && e.detail.payload.size.sizeAvailableInStores === false;\n                    }\n  \n                    setTimeout(function() { \n                      _satellite.track('product:size', {\n                        sku : prodObj.sku, \n                        size : sizeValue,\n                        bossDisabled : sizeBOSSDisabled\n                      });\n                    }, dctfl.eventDelay);\n                    setTimeout(function() { \n                      _satellite.track('affiliate:product:size', {\n                        sku : prodObj.sku, \n                        size : sizeValue,\n                        bossDisabled : sizeBOSSDisabled\n                      });\n                    }, dctfl.affiliateDelay);\n  \n                    // set last size value\n                    dctfl.addPageInteraction('pdpSizeSelected', e.detail.payload.size.size.value);\n                    \n                  } else {\n                    dctfl.log('@product style event source : PRODUCT_DETAILS_SELECT_VALUE');\n  \n                    var prevStyleSku = '';\n                    if (pdpStyleSkus && pdpStyleSkus.length > 0) {\n                      prevStyleSku = pdpStyleSkus[pdpStyleSkus.length-1];\n                      dctfl.log('@product sku prev : ' + prevStyleSku + ' : current : ' + prodObj.sku);\n                      if (prevStyleSku === prodObj.sku) {\n                        sendProductEvents = false;\n                        dctfl.log('@product style change same sku');\n                      }\n                    }\n  \n                    if (sendProductEvents) {\n    \n                      // track the product style changes\n                      dctfl.addPageInteraction('pdpSkus', prodObj.sku);\n  \n                      // check if style change is from age bucket change\n                      // ensure that the sku isn't part of the existing styles\n                      var previousProductAges = dctfl.lastViewedPage['prodViewAgeBuckets'] || [],\n                          previousProdStyles = dctfl.lastViewedPage['prodViewStyles'] || [],\n                          styleChangeFromAge = false;\n                      if (previousProductAges.length > 0 && previousProdStyles.length > 0) {\n                        var styleSkuFound = false;\n                        for (var i=0; i<previousProdStyles.length; i++) {\n                          if (previousProdStyles[i].sku === prodObj.sku) {\n                            styleSkuFound = true;\n                            break;\n                          }\n                        }\n  \n                        if (styleSkuFound === false) {\n                          for (var i=0; i<previousProductAges.length; i++) {\n                            if (previousProductAges[i].sku === prodObj.sku) {\n                              styleChangeFromAge = true;\n                              break;\n                            }\n                          }\n                        }\n                      }\n  \n                      dctfl.log('@product redux styleChangeFromAge', styleChangeFromAge); \n  \n                      // product page skyle sku change event\n                      setTimeout(function() { \n                        _satellite.track('product:style', {\n                          pageType : 'product',\n                          isPageLoad : isPageLoad,\n                          styleSkuChange : true,\n                          styleSkuAgeChange : styleChangeFromAge,\n                          sku : prodObj.sku,\n                          productData : productData\n                        });\n                      }, dctfl.eventDelay);\n          \n                      // product page skyle sku change event for affiliate libraries\n                      setTimeout(function() { \n                        _satellite.track('affiliate:product:style', {\n                          pageType : 'product',\n                          isPageLoad : isPageLoad,\n                          styleSkuChange : true,\n                          styleSkuAgeChange : styleChangeFromAge,\n                          sku : prodObj.sku,\n                          productData : productData\n                        });\n                      }, dctfl.affiliateDelay);\n  \n                      setTimeout(function() {\n                        // reset product style and age data\n                        // delay the call to ensure the state is current\n                        var obj = _satellite.getVar('getProductDetails');\n                        if (obj) {\n                          dctfl.log('@product prodView reseting style data');\n                          dctfl.lastViewedPage['prodViewAgeBuckets'] = obj.ageBuckets;\n                          if (styleChangeFromAge) {\n                            dctfl.lastViewedPage['prodViewStyles'] = obj.styles;\n                          }\n                          var event = new CustomEvent(\n                            'analyticsEvent', {\n                                detail: {\n                                  eventName: 'productView',\n                                  payload: {\n                                    pageName: 'PDP',\n                                    productSku: obj.sku,\n                                    productName: obj.name,\n                                    productStyle: obj.style,\n                                    productCategories: obj.categories,\n                                    modelId: obj.modelNumber,\n                                    styleChange: true\n                                  }\n                                },\n                                bubbles: true,\n                                cancelable: true\n                          });          \n                          window.dispatchEvent(event);\n                        }\n                      }, dctfl.eventDelay);\n  \n                      \n  \n                    }\n        \n                  }\n  \n                }\n              }\n  \n            }\n  \n          } \n        } \n        break;\n  \n      // DD-3222 New PDP API:\n      case '@product_details/GET_DETAILS':\n        if (e.detail.payload && e.detail.payload.style) {\n  \n          var sendProductEvents = true,\n              sku = e.detail.payload.style.sku,\n              prevStyleSku = '',\n              pdpStyleSkus = dctfl.getPageInteractions('pdpSkus'),\n              isPageLoad = dctfl.locationCount > 1 ? false : true;\n  \n          if (sku) {\n  \n            if (pdpStyleSkus && pdpStyleSkus.length > 0) {\n              prevStyleSku = pdpStyleSkus[pdpStyleSkus.length-1];\n              dctfl.log('@product sku prev : ' + prevStyleSku + ' : current : ' + sku);\n              if (prevStyleSku === sku) {\n                sendProductEvents = false;\n                dctfl.log('@product style change same sku');\n              }\n            }\n  \n            if (dctfl.debug) {\n              dctfl.log('@product GET_DETAILS lastViewedProduct: ' + dctfl.lastViewedPage['prodView']);\n              dctfl.log('@product GET_DETAILS getProductSKUfromURL: ' + _satellite.getVar('getProductSKUfromURL'));\n            }\n  \n            if (!dctfl.lastViewedPage['prodView'] || sku === dctfl.lastViewedPage['prodView']) {\n  \n              if (sku === dctfl.lastViewedPage['prodView']) {\n                var skuInteractions = dctfl.getPageInteractions('pdpSkus') || [];\n                if (skuInteractions.length > 0) {\n                  var lastSku = skuInteractions[skuInteractions.length-1];\n                  if (lastSku === sku) {\n                    // ignore the duplicate style change\n                    sendProductEvents = false;\n                  }\n                } else {\n                  // no previous skus, do not send style change\n                  sendProductEvents = false;\n                }\n              } else {\n                // ignore initial api response as we handle via the dctfl.prodView mechanism\n                sendProductEvents = false;\n              }\n            }\n  \n            if (sendProductEvents) {\n  \n              dctfl.log('@product style event source : GET_DETAILS');\n    \n              // track the product style changes\n              dctfl.addPageInteraction('pdpSkus', sku);\n  \n              // check if style change is from age bucket change\n              // ensure that the sku isn't part of the existing styles\n              var previousProductAges = dctfl.lastViewedPage['prodViewAgeBuckets'] || [],\n                  previousProdStyles = dctfl.lastViewedPage['prodViewStyles'] || [],\n                  styleChangeFromAge = false;\n  \n              if (previousProductAges.length > 0 && previousProdStyles.length > 0) {\n                \n                var styleSkuFound = false;\n                for (var i=0; i<previousProdStyles.length; i++) {\n                  if (previousProdStyles[i].sku === sku) {\n                    styleSkuFound = true;\n                    break;\n                  }\n                }\n  \n                if (styleSkuFound === false) {\n                  for (i=0; i<previousProductAges.length; i++) {\n                    if (previousProductAges[i].sku === sku) {\n                      styleChangeFromAge = true;\n                      break;\n                    }\n                  }\n                }             \n              \n              }\n  \n              dctfl.log('@product styleChangeFromAge', styleChangeFromAge); \n  \n              // product page skyle sku change event\n              setTimeout(function() { \n                _satellite.track('product:style', {\n                  pageType : 'product',\n                  isPageLoad : isPageLoad,\n                  styleSkuChange : true,\n                  styleSkuAgeChange : styleChangeFromAge,\n                  sku : sku,\n                });\n              }, dctfl.eventDelay);\n  \n              // product page skyle sku change event for affiliate libraries\n              setTimeout(function() { \n                _satellite.track('affiliate:product:style', {\n                  pageType : 'product',\n                  isPageLoad : isPageLoad,\n                  styleSkuChange : true,\n                  styleSkuAgeChange : styleChangeFromAge,\n                  sku : sku\n                });\n              }, dctfl.affiliateDelay);\n  \n              setTimeout(function() {\n                var productData = _satellite.getVar('getProductDetails');\n                if (productData) {\n                  dctfl.log('@product GET_DETAILS reseting style data');\n                  dctfl.lastViewedPage['prodViewAgeBuckets'] = productData.ageBuckets;\n                  if (styleChangeFromAge) {\n                    dctfl.lastViewedPage['prodViewStyles'] = productData.styles;\n                  }\n                  var event = new CustomEvent(\n                    'analyticsEvent', {\n                      detail: {\n                        eventName: 'productView',\n                        payload: {\n                          pageName: 'PDP',\n                          productSku: productData.sku,\n                          productName: productData.name,\n                          productStyle: productData.style,\n                          productCategories: productData.categories,\n                          modelId: productData.modelNumber,\n                          styleChange: true\n                        }\n                      },\n                      bubbles: true,\n                      cancelable: true\n                    }\n                  );    \n                }      \n                window.dispatchEvent(event);\n              }, dctfl.eventDelay);\n  \n            } else {\n              dctfl.log('@product GET_DETAILS not sending style change');\n            }\n          }\n        \n        } else {\n          dctfl.log('@product GET_DETAILS no payload');\n        }\n  \n        break;\n      case '@product_details/ORDER_STYLES':\n        setTimeout(function() {\n          // reset product style and age data\n          var obj = _satellite.getVar('getProductDetails');\n          if (obj) {\n            dctfl.log('@product ORDER_STYLES reseting style data');\n            dctfl.lastViewedPage['prodViewAgeBuckets'] = obj.ageBuckets;\n            if (styleChangeFromAge) {\n              dctfl.lastViewedPage['prodViewStyles'] = obj.styles;\n            }\n          }\n        }, dctfl.eventDelay);\n        break;\n      case '@product_details/SELECT_VALUE':\n        if (e.detail.payload && e.detail.payload.size) {\n  \n          var selectedStyle = e.detail.payload.style,\n              state = footlocker.globalstore.getState(),\n              size = e.detail.payload.size;\n  \n          if (location.pathname in state.features.pdp.styles && state.features.pdp.styles[location.pathname][selectedStyle]) {\n            var sku = state.features.pdp.styles[location.pathname][selectedStyle].sku;\n            if (sku) {\n  \n              var prevSize = '',\n                  pdpSizesSelected = dctfl.getPageInteractions('pdpSizeSelected'),\n                  sendSizeEvent = true;\n  \n              // ensure we are not sending duplicate size change events\n              if (pdpSizesSelected && pdpSizesSelected.length > 0) {\n                prevSize = pdpSizesSelected[pdpSizesSelected.length-1];\n                dctfl.log('@product size : ' + prevSize + ' : current : ' + size);\n                if (prevSize === size) {\n                  sendSizeEvent = false;\n                  dctfl.log('@product size change same size');\n                }\n              }\n  \n              if (sendSizeEvent) {\n  \n                dctfl.log('@product size event source : SELECT_VALUE');\n                dctfl.log('@product size selected', selectedStyle, sku, size);\n  \n                // set last size value\n                dctfl.addPageInteraction('pdpSizeSelected', size);\n  \n                setTimeout(function() { \n                  _satellite.track('product:size', {\n                    sku : sku, \n                    size : size,\n                    bossDisabled : false // TODO future requirement\n                  });\n                }, dctfl.eventDelay);\n                setTimeout(function() { \n                  _satellite.track('affiliate:product:size', {\n                    sku : sku, \n                    size : size,\n                    bossDisabled : false // TODO future requirement\n                  });\n                }, dctfl.affiliateDelay); \n  \n              }\n  \n            }\n          }\n        }\n        break;\n      case 'PRODUCT_DETAILS_RESET_LAUNCH_PRODUCT':\n        _satellite.track('pdp:launch:reset');\n        break;\n      case 'PRODUCT_DETAILS_SET_STYLE':\n        break;\n      case 'EVENT_ADD_FUSION':\n        // fusion search query event\n        if (e.detail.payload && e.detail.payload.type && e.detail.payload.type === 'search') {\n          window.digitalData.searchResults = window.digitalData.searchResults || {};\n          if (e.detail.payload.consumer) {\n            window.digitalData.searchResults.consumer = e.detail.payload.consumer;\n          }\n          if (e.detail.payload.query) {\n            window.digitalData.searchResults.query = e.detail.payload.query;\n            try {\n              localStorage.setItem('google360SearchTerm', e.detail.payload.query);\n            } catch (err) { dctfl.log(err); }\n            setTimeout(function() {\n              _satellite.track('search:userinitiated', { query : e.detail.payload.query });\n            }, dctfl.eventDelay);\n            setTimeout(function() {\n              _satellite.track('affiliate:search:userinitiated', { query : e.detail.payload.query });\n            }, dctfl.affiliateDelay);\n          }\n        }\n        break;\n      case 'PRODUCT_SEARCH_SUGGESTIONS':\n        _satellite.track('search_suggest');\n        break;\n      case 'SET_PREVIOUS_HISTORY':\n        if (typeof dctfl.lastViewedPage === 'object') {\n          if ('pageView' in dctfl.lastViewedPage) {\n            // reset the pageView state to handle scenarios where the prevs and next path are both /search\n            dctfl.log('@pageView resetting dctfl.lastViewedPage[pageView]');\n            dctfl.lastViewedPage['pageView'] = '';\n          }\n          if ('prodView' in dctfl.lastViewedPage) {\n            dctfl.log('@product resetting dctfl.lastViewedPage[prodView]');\n            delete dctfl.lastViewedPage['prodView'];\n          }\n        }\n        break;\n      case 'PRODUCT_SEARCH_SUGGESTIONS_RESET':\n        _satellite.track('search_suggest_reset');\n        break;\n      case 'PRODUCT_SEARCH_SUCCESS':\n        window.digitalData.searchResults = window.digitalData.searchResults || {};\n        var products = JSON.parse(JSON.stringify(e.detail.payload.products)),\n            sorts = JSON.parse(JSON.stringify(e.detail.payload.sorts));\n        var queryID = false;\n        if (typeof e.detail.payload.queryID === 'string') {\n            queryID = e.detail.payload.queryID;\n        }\n        if (e.detail.payload.merge !== true) {\n          digitalData.searchResults.products = products;\n        } else {\n          try {\n            digitalData.searchResults.products.push.apply(digitalData.searchResults.products, products);\n          } catch(error) {}\n        }\n        \n        digitalData.searchResults.sorts = sorts;\n   \n        // store search\n        if ('metaData' in e.detail.payload && 'storeProductSearch' in e.detail.payload.metaData) {\n            digitalData.searchResults.storeSearch = e.detail.payload.metaData.storeProductSearch ? true : false;\n        }\n        \n        if (queryID) {\n          digitalData.searchResults.queryID = queryID;\n        }\n        break;\n      case 'LISTS_CREATE_LIST':\n        setTimeout(function() { \n          _satellite.track('wishlist:create',{\n            payload:e.detail.payload\n          });\n        }, dctfl.eventDelay);      \n        break;\n      case 'LISTS_ADD_PRODUCT':\n        var sku = '';\n        if (e.detail.payload && 'styleCode' in e.detail.payload) {\n          sku = e.detail.payload.styleCode || '';\n          if (sku) {\n            setTimeout(function() { \n              _satellite.track('wishlist:add',{ \n                sku : sku\n              });\n            }, dctfl.eventDelay); \n          }\n        }      \n        break;\n      case 'CART_FETCHED':\n        var pageType = _satellite.getVar('pageType');\n        pageType = (pageType ? pageType.toLowerCase() : '');\n        if (pageType === 'cart') {\n          // cart page\n          setTimeout(function() { \n            _satellite.track('cart', {\n              pageType : 'cart'\n            });\n          }, dctfl.eventDelay);\n  \n          // cart page for affiliate libraries\n          setTimeout(function() { \n            _satellite.track('affiliate:cart', {\n              pageType : 'cart'\n            });\n          }, dctfl.affiliateDelay);\n        }\n  \n        try {\n          // bubble up billing email\n          var paymentAddress = e.detail.payload.paymentAddress;\n          if (paymentAddress && paymentAddress.email && paymentAddress.email.indexOf('@') > -1) {\n            _satellite.track('cart:billingemail', {\n              email : paymentAddress.email\n            });\n          }\n        } catch (e) {\n          dctfl.log(e);\n        }\n  \n        break;\n      case 'CART_ENTRY_ADDED':\n        // cart add\n        if (!e.detail.payload.message && !e.detail.payload.errors) {\n          var pageType = _satellite.getVar('pageType') || '',\n              productInfo = _satellite.getVar('getProductDetails') || {},\n              cartItems = e.detail.payload.entries,\n              cartItem;\n  \n          if (pageType.toLowerCase() !== 'cart' && pageType.toLowerCase() !== 'checkout') {\n            // Get the Quantity\n            try {\n              var qtyInput = document.querySelector('input[name=quantity]');\n              productInfo.qty = qtyInput && 'value' in qtyInput ? parseInt(qtyInput.value) : 1;\n              \n              if('qty' in productInfo) productInfo.price = parseFloat(productInfo.qty * productInfo.price).toFixed(2);\n            } catch(e) {\n              dctfl.log(e);\n            }\n            \n            if (e.detail.payload.cartCreated === true) {\n              setTimeout(function() { \n                _satellite.track('cart:add:created', {\n                  cartCreated: true,\n                  payload: e.detail.payload,\n                  product: productInfo\n                });\n              }, dctfl.eventDelay);\n            } else {\n              setTimeout(function() { \n                _satellite.track('cart:add', {\n                  cartCreated: false,\n                  payload: e.detail.payload,\n                  product: productInfo\n                });\n              }, dctfl.eventDelay);\n            }\n            setTimeout(function() { \n              _satellite.track('affiliate:cart:add', {\n                cartCreated: e.detail.payload.cartCreated,\n                payload: e.detail.payload,\n                product: productInfo\n              });\n            }, dctfl.affiliateDelay);\n  \n          } else {\n            var theState = {};\n            \n            try {\n              theState = footlocker.globalstore.getState();\n  \n              var currentItemCode = e.detail.payload.code,\n                  currentCartItems = 'entries' in theState.cart.current ? theState.cart.current.entries : {},\n                  itemInCart = false,\n                  itemQtyDecrease,\n                  currentCartItem;\n              \n              // Grab the cartItem\n              for (var i = 0; i < cartItems.length; i++) {\n                cartItem = cartItems[i];\n  \n                if(currentItemCode == cartItem.product.code) {\n                  for (var ie = 0; ie < currentCartItems.length; ie++) {\n                    currentCartItem = currentCartItems[ie];\n  \n                    if(currentCartItem.product.code == cartItem.product.code) {\n                      itemInCart = true;\n                      if(cartItem.quantity > 0 && currentCartItem.quantity > 0 && cartItem.quantity != currentCartItem.quantity) itemQtyDecrease = cartItem.quantity > currentCartItem.quantity ? false : true;\n                      break;\n                    }\n                  }\n  \n                  break;\n                }\n              }\n              \n              dctfl.log('@@ cart redux: ', cartItem, currentCartItem);\n              \n              // Get the SKU\n              if(typeof cartItem == 'object' && 'product' in cartItem) {\n                for(var i = 0; i < cartItem.product.baseOptions.length; i++) {\n                  var baseOption = cartItem.product.baseOptions[i].selected;\n                  if(typeof baseOption === 'object' && 'sku' in baseOption) cartItem.sku = baseOption.sku;\n                  if(typeof baseOption === 'object' && 'size' in baseOption) cartItem.size = baseOption.size;\n                }\n              }\n              \n              if(!('sku' in cartItem) || !('size' in cartItem)) {\n                if(typeof currentCartItem == 'object' && 'product' in currentCartItem) {\n                  for(var i = 0; i < currentCartItem.product.baseOptions.length; i++) {\n                    var baseOption = currentCartItem.product.baseOptions[i].selected;\n                    if(typeof baseOption === 'object' && 'sku' in baseOption) cartItem.sku = baseOption.sku;\n                    if(typeof baseOption === 'object' && 'size' in baseOption) cartItem.size = baseOption.size;\n                  }\n                }\n              }\n              \n              if('sku' in cartItem) {\n                if(typeof currentCartItems === 'object' && currentCartItems.length <= 0) {\n                  // New Item in New Cart\n                  dctfl.log('New Item, New Cart');\n                setTimeout(function() {\n                    var product = {\n                        sku: cartItem.sku || '',\n                        qty: cartItem.quantity,\n                        price: cartItem.totalPrice.value,\n                        size: cartItem.size\n                      }\n                    if (isEUorAPAC){\n                        product.priceWtax = product.price;\n                        product.price = cartItem.priceNet.value\n                    }\n                    checkCartStateUpdate(10, fireDCR)\n\n                    function fireDCR(){\n                        _satellite.track('cart:add:saved', {\n                            cartCreated: e.detail.payload.cartCreated === true ? true : false,\n                            payload: e.detail.payload,\n                            inCart: true,\n                            product: product,\n                        });\n                    }\n                  \n                  \n                  }, dctfl.eventDelay);\n                  setTimeout(function() {\n                    var product = {\n                        sku: cartItem.sku || '',\n                        qty: cartItem.quantity,\n                        price: cartItem.totalPrice.value,\n                        size: cartItem.size\n                    }\n                    if (isEUorAPAC){\n                        product.priceWtax = product.price;\n                        product.price = cartItem.priceNet.value  * cartItem.quantity\n                    }\n                    _satellite.track('affiliate:cart:add:saved', {\n                      cartCreated: e.detail.payload.cartCreated === true ? true : false,\n                      payload: e.detail.payload,\n                      inCart: true,\n                      product: product\n                    });\n                  }, dctfl.affiliateDelay);\n                } else {   \n                  if(itemInCart) {\n                    if(itemQtyDecrease == false) {\n                      // Quantity Increase\n                      dctfl.log('Qty Increase');\n                      \n                      var qtyDelta = cartItem.quantity - currentCartItem.quantity;\n                      setTimeout(function() {\n                        var product = {\n                          sku: cartItem.sku || '',\n                          qty: qtyDelta,\n                          price: (cartItem.totalPrice.value / cartItem.quantity) * qtyDelta,\n                          size: cartItem.size || ''\n                        }          \n                        if (isEUorAPAC){\n                            product.priceWtax = product.price;\n                            product.price = cartItem.priceNet.value\n                        }\n                        checkDDupdate(10, fireDCR)\n                     \n                        function fireDCR(){\n                            _satellite.track('cart:edit:increase', {\n                                cartCreated: e.detail.payload.cartCreated === true ? true : false,\n                                payload: e.detail.payload,\n                                inCart: true,\n                                product : product\n                            });\n                        }\n                      }, dctfl.eventDelay);\n                      setTimeout(function() {\n                         var product = {\n                          sku: cartItem.sku || '',\n                          qty: qtyDelta,\n                          price: cartItem.totalPrice.value * qtyDelta,\n                          size: cartItem.size || ''\n                        }\n                        if (isEUorAPAC){\n                           product.priceWtax = product.price;\n                           product.price = cartItem.priceNet.value * qtyDelta\n                        }\n                        _satellite.track('affiliate:cart:edit:increase', {\n                          cartCreated: e.detail.payload.cartCreated === true ? true : false,\n                          payload: e.detail.payload,\n                          inCart: true,\n                          product:product\n                        });\n                      }, dctfl.affiliateDelay);\n                    } else if(itemQtyDecrease == true) {\n                      // Quantity Decrease\n                      dctfl.log('Qty Decrease');\n                      \n                      var removedProduct = e.detail.payload.entries.find(function(item) {\n                        return item.product.code == e.detail.payload.code;\n                      });\n                      \n                      var qtyDelta = currentCartItem.quantity - cartItem.quantity;\n                      \n                      removedProduct.quantity = qtyDelta;\n                      removedProduct.totalPrice.value = (parseFloat(currentCartItem.totalPrice.value) / currentCartItem.quantity) * qtyDelta;\n                      checkDDupdate(10, fireDCR)\n                     \n                      function fireDCR(){\n                        setTimeout(function() {\n                            _satellite.track('cart:remove', removedProduct);\n                        }, dctfl.eventDelay);\n                      }\n                    \n                      setTimeout(function() {\n                        _satellite.track('affiliate:cart:remove', removedProduct);\n                      }, dctfl.affiliateDelay);\n                    } else {\n                      // Item in Cart, No Qty Change\n                      dctfl.log('Item in Cart, Something else...');\n                      setTimeout(function() {\n                        _satellite.track('cart:change:fulfillment', {\n                          payload: e.detail.payload,\n                          product: cartItem\n                        });\n                      }, dctfl.eventDelay);\n                    }\n                  } else {\n                    // Check Saved For Later\n                    var inSavedItems = false,\n                        savedItems = theState.savedForLater.products;\n                    \n                    if(savedItems.length >= 1) {\n                      for(var i = 0; i < savedItems.length; i++) {\n                        var savedItem = savedItems[i];\n                        \n                        if(savedItem.sku == cartItem.sku) {\n                          inSavedItems = true;\n                          break;\n                        }\n                      }\n                    }\n                    \n                    if(inSavedItems) {\n                      // New Item in Existing Cart\n                      dctfl.log('New Item, Existing Cart');\n                      \n                      setTimeout(function() {\n                        var product =  {\n                            sku: cartItem.sku || '',\n                            qty: cartItem.quantity,\n                            price: cartItem.totalPrice.value,\n                            size: cartItem.size || ''\n                          }\n                        if (isEUorAPAC){\n                            product.priceWtax = p