           messageCategory = 'cheetahmail';\n        }\n      } else if (e.origin.indexOf('paypal') != -1) {\n        // paypal checkout & paypal credit\n        if ( e.data.indexOf('\"fundingSource\": \"paypal\"') != -1) {\n              messageCategory = 'paypalCheckout';\n        } else if ( e.data.indexOf('\"fundingSource\": \"credit\"') != -1) {\n            messageCategory = 'paypalCredit';\n        }\n      }\n\n      switch (messageCategory) {\n        case 'crowdtwist':\n          // crowdtwist event tracking\n          var ctdata = JSON.parse(e.data);\n          var ctEventData = ctdata.eventData || {};\n          if (ctdata.eventName) {\n            dctfl.log('@crowdtwist event : ' + ctdata.eventName, ctdata);\n            switch (ctdata.eventName) {\n              case 'modal:open':\n                var ctState = ctEventData.state || '';\n                _satellite.track('crowdtwist:modal:open', {\n                  state : ctState\n                });\n                break;\n              case 'reward:redeemed':\n                _satellite.track('crowdtwist:reward:redeemed', {\n                  rewardTitle : ctEventData.rewardTitle,\n                });\n                break;\n              case 'navigate':\n                _satellite.track('crowdtwist:navigate', { url : ctEventData.url || '' });\n                break;\n            }\n          }\n          break;\n\n        case 'cheetahmail':\n          dctfl.log('@cheetmail event', e.data);\n          if (e.data === 'submit') {\n            // legacy submit event without md5\n            _satellite.track('email:signup', { modalId : 'marketingModal'});\n          } else if (e.data.indexOf('eventData') > -1) {\n            // parse eventData\n            var data = JSON.parse(e.data);\n            var eventData = data.eventData;\n            if (eventData && eventData.eventName) {\n              if (eventData.eventName === 'email signup') {\n                _satellite.track('email:signup', { emailMd5 : eventData.emailMd5, modalId : 'marketingModal' });\n              }\n            }\n          }\n          break;\n\n        case 'paypalCheckout':\n          dctfl.log('@paypalCheckout event', e.data);\n            _satellite.track('paypal:checkout');\n          break;\n\n        case 'paypalCredit':\n          dctfl.log('@paypalCredit event', e.data);\n            _satellite.track('paypal:credit');\n          break;\n      }      \n    }\n\n  } catch (err) {\n    dctfl.log(err);\n  }\n});",language:"javascript"}}]},{id:"RL43b8f59adc404beab5a696ce7a81efc7",name:"DCR: affiliate:cart:add Google",events:[{modulePath:"core/src/lib/events/directCall.js",settings:{identifier:"affiliate:cart:add"},ruleOrder:52}],conditions:[{modulePath:"core/src/lib/conditions/customCode.js",settings:{source:function(){return!0===dctfl.affiliateCheck(dctfl.domain,"google")}}},{modulePath:"core/src/lib/conditions/valueComparison.js",settings:{comparison:{operator:"isTrue"},leftOperand:"%TargetingCookiesEnabled%"}}],actions:[{modulePath:"core/src/lib/actions/customCode.js",settings:{source:"https://assets.adobedtm.com/b714f0d8126e/c733dac6d95d/720b2c88259b/RCae71d64082064fdaa94b22801b9ebda6-source.min.js",language:"javascript",isExternal:!0}}]},{id:"RL3d8beddf9f084b2296ff6dffd1bad7f1",name:"DCR: affiliate:locationChange Facebook",events:[{modulePath:"core/src/lib/events/directCall.js",settings:{identifier:"affiliate:locationChange"},ruleOrder:25}],conditions:[{modulePath:"core/src/lib/conditions/valueComparison.js",settings:{comparison:{operator:"isTrue"},leftOperand:"%TargetingCookiesEnabled%"}},{modulePath:"core/src/lib/conditions/customCode.js",settings:{source:function(){return!0===dctfl.affiliateCheck(dctfl.domain,"facebook")}}},{modulePath:"core/src/lib/conditions/customCode.js",settings:{source:function(e){return!1===e.detail.isPageLoad}}}],actions:[{modulePath:"core/src/lib/actions/customCode.js",settings:{source:"https://assets.adobedtm.com/b714f0d8126e/c733dac6d95d/720b2c8825