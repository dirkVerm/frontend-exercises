e) {\n  if (e.detail.type === 'recommendations' || e.detail.type === 'recentItems' || e.detail.type === 'newArrivals' || e.detail.type === 'moreDeals') {\n    if (e.detail.payload.seenCount === 1) {\n      _satellite.track('adobetarget:recs:impression', {\n          recs : e.detail.payload.data,\n          zoneName : e.detail.type\n      });\n    }\n  }\n});\n\n\nwindow.addEventListener('digitalDataUpdate', function(e) {\n  var type = e.detail.payload.keyUpdates,\n      event = e.detail.payload.reduxEvent;\n  dctfl.log(e);\n  switch(type) {\n    case 'page.pageInfo': \n      setTimeout(function() {\n        dctfl.pageView(e.detail.payload.page.pageInfo.pageName);\n      }, dctfl.pageDelay);\n      if(dctfl.locationCount) isPageLoad = (dctfl.locationCount > 1 ? false : true);\n      break;\n    case 'search': \n      var pageType = _satellite.getVar('pageType') || '';\n      pageType = pageType.toLowerCase();\n      var searchObj = _satellite.getVar('getSearchDetails');\n      if (searchObj && \n          dctfl.findInList(pageType, [\n            'search', \n            'category', \n            'brand', \n            'subcategory', \n            'subsubcategory', \n            'subsubsubcategory',\n            'supercategory',\n            'null search'])) {\n        if ((event === 'PRODUCT_SEARCH_SUCCESS' || event === 'PRODUCT_SEARCH_SET') && typeof digitalData.search.results !== 'undefined') {\n          dctfl.log('Search event: ' + event);\n          dctfl.log('digitalData.search.results: ' + digitalData.search.results);\n\n          var isPageNotFound = false;\n          if (digitalData.notFound && digitalData.notFound.path && digitalData.notFound.path === location.pathname && digitalData.notFound.status === true) {\n            dctfl.log('Search event: ' + event + ' results from 404 page, skipping DCR');\n            window.digitalData.searchResults = {};\n            isPageNotFound = true;\n            _satellite.track('pageNotFound')\n          }\n\n         